{
  "name": "next-collect",
  "version": "0.0.0",
  "files": [
    "./dist/*"
  ],
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "prettier:check": "prettier --check --config ../common-configs/.prettierrc.json --ignore-path ../common-configs/.prettierignore .",
    "prettier:fix": "prettier --write --config ../common-configs/.prettierrc.json --ignore-path ../common-configs/.prettierignore .",
    "build": "tsc -p .",
    "test": "jest",
    "clean": "rm -rf ./next",
    "dev": "tsc -p . --watch --sourceMap --declarationMap",
    "lint": "yarn prettier:check"
  },
  "dependencies": {
    "cookie": "^0.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/cookie": "^0.5.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "common-configs": "workspace:*",
    "jest": "~28.1.0",
    "next": "^12.1.6",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18",
    "react-dom": "^17.0.2 || ^18"
  }
}
