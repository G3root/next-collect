{
  "pipeline": {
    "test": {
      "dependsOn": ["build","^test"]
    },
    "clean": {
      "dependsOn": ["^clean"]
    },
    "build": {
      "dependsOn": ["^build", "clean", "lint"],
      "outputs": ["dist/**", ".next/**"]
    },
    "prettier:fix": {
      "dependsOn": ["^prettier:fix"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
